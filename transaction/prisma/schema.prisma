generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id                      String    @id @default(uuid())
  accountExternalIdCredit String?
  accountExternalIdDebit  String?
  status                  String
  tranferTypeId           Int
  value                   Int
  createdAt               DateTime  @default(now())
  updatedAt               DateTime?
}
